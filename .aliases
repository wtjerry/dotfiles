alias o=xdg-open

alias cd..='cd ..'
alias ll='ls -l'
alias la='ls -lA'
alias mkdir='mkdir -p'

alias ls='ls --color=auto'

alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias gdt='git difftool -y'
alias gits='git status -su'
